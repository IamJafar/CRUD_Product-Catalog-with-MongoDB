    "# Importing required libraries\n",
    "\n",
    "import pymongo\n",
    "\n",
    "# creating connection with moongoDb Atlas\n",
    "client = pymongo.MongoClient(\"your unique client id") \n",
    "\n",
    "# Creating New Database in MongoDB\n",
    "db = client.myproject\n",
    "\n",
    "# Creating a new Collection named \"productcatalog\"\n",
    "mycol = db.productcatalog\n",
    "\n",
    "\n",
    "# Module 1 INSERTION\n",
    "def insert():\n",
    "    #Getting user input in a correct way\n",
    "    while True:\n",
    "        try:\n",
    "            N = int(input(\"How many products you need to insert : \"))   \n",
    "        except:\n",
    "            print(\"An error occured!! Please Enter an integer\")\n",
    "            continue\n",
    "        else:\n",
    "            break       \n",
    "            \n",
    "    #using for loop if multiple product details needs to be inserted\n",
    "    for i in range(1,N+1):\n",
    "        \n",
    "        print('\\nPlease Enter below the details for Product No. {}\\n'.format(i))\n",
    "        \n",
    "        prod_list = {\"product_id\" : int(input('Enter product_id : ')),\n",
    "                     \"name\" : input('Enter Product_name : '),\n",
    "                     \"description\" : input('Enter Product_description : '),\n",
    "                     \"price\" : int(input('Enter product_price : ')),\n",
    "                     \"category\" : input('Enter Product_category : '),\n",
    "                     \"discount\" : int(input('Enter Discount_percentage : '))\n",
    "                    }\n",
    "        #inserting all the user entered details using insert_one() method\n",
    "        mycol.insert_one(prod_list)\n",
    "        \n",
    "    return f\"\\nSuccessfully Inserted the product details!!!\"\n",
    "\n",
    "\n",
    "# Module 2 UPDATE\n",
    "def update():\n",
    "    while True:\n",
    "        try:\n",
    "            #First making the user to select the product_id to be updated\n",
    "            userinp = {\"product_id\" : int(input('Enter the product_id to perform updation : '))}\n",
    "        except:\n",
    "            print(\"\\nPlease Enter a valid Product_id!\")\n",
    "        else:\n",
    "            break\n",
    "            \n",
    "    # Then making user to select the category--cat\n",
    "    cat  = input(\"\\nWhich detail you need to update? : \").lower()\n",
    "    \n",
    "    while cat not in [\"name\", \"product_id\", \"description\",\"price\", \"category\", \"discount\"]:\n",
    "        try:\n",
    "            print(\"\\nPlease Enter any one of the field properly as shown in below list!\")\n",
    "            print([\"name\", \"product_id\", \"description\",\"price\", \"category\", \"discount\"])\n",
    "            cat  = input(\"Which detail you need to update? : \").lower()\n",
    "        except:\n",
    "            pass\n",
    "        \n",
    "    # Validating user command with the help of IF Else condition\n",
    "    if cat == \"name\":\n",
    "        new = {\"$set\" : {cat : input(\"Enter new product_name : \")}}\n",
    "        mycol.update_one(userinp, new)\n",
    "    elif cat == \"product_id\":\n",
    "        new = {\"$set\" : {cat : int(input(\"Enter new product_id : \"))}}\n",
    "        mycol.update_one(userinp, new)\n",
    "    elif cat == \"description\":\n",
    "        new = {\"$set\" : {cat : input(\"Enter new product_description : \")}}\n",
    "        mycol.update_one(userinp, new)\n",
    "    elif cat == \"price\":\n",
    "        new = {\"$set\" : {cat : int(input(\"Enter new price : \"))}}\n",
    "        mycol.update_one(userinp, new)\n",
    "    elif cat == \"category\":\n",
    "        new = {\"$set\" : {cat : input(\"Enter new product_category : \")}}\n",
    "        mycol.update_one(userinp, new)\n",
    "    elif cat == \"discount\":\n",
    "        new = {\"$set\" : {cat : int(input(\"Enter new product_discount : \"))}}\n",
    "        mycol.update_one(userinp, new)\n",
    "    else:\n",
    "        print(\"You have entered incorrectly Try again!!!\")\n",
    "    \n",
    "    return f\"\\nSuccessfully Updated!!!\"\n",
    "\n",
    "\n",
    "# Module 3 DELETE\n",
    "def delete():\n",
    "    userinp = {\"product_id\" : int(input('Please mention the product_id to delete : '))}\n",
    "    mycol.delete_one(userinp)\n",
    "    return f\"\\nDeleted!!!\"\n",
    "\n",
    "\n",
    "# Module 4 READ\n",
    "#Reading all the data available in the collections\n",
    "def read():\n",
    "    for i in mycol.find().sort('product_id'):\n",
    "        print(i,'\\n')\n",
    "    return\n",
    "\n",
    "# Module 5 FILTER\n",
    "def filtering():\n",
    "    us_filter = input(\"Which filter you need to apply (name/category/price)? : \").lower()\n",
    "\n",
    "    if us_filter == \"name\":\n",
    "        for i in mycol.find({us_filter : input(\"Enter name of the product you are looking for : \")}):\n",
    "            print('\\n',i)\n",
    "    elif us_filter == \"category\":\n",
    "        for i in mycol.find({us_filter : input(\"Enter the product category you are looking for : \")}):\n",
    "            print('\\n',i)\n",
    "    elif us_filter == \"price\":\n",
    "        for i in mycol.find({us_filter : {'$gte' : int(input(\"Enter the price you are looking for : \"))}}):\n",
    "            print('\\n',i)\n",
    "\n",
    "\n",
    "# Driver code to execute all the functions\n",
    "\n",
    "print(\"Hello there ðŸ™‹\\n\")\n",
    "userchoice = input(\"\\nPlease enter which operation to be performed : \")\n",
    "while userchoice not in [\"insert\", \"update\", \"delete\",\"read\", \"filter\"]:\n",
    "    try:\n",
    "        print(\"\\nPlease Enter any one of the operation properly as shown in below list!\")\n",
    "        print([\"insert\", \"update\", \"delete\",\"read\", \"filter\"])\n",
    "        userchoice  = input(\"Which operation need to perform? : \\n\").lower()\n",
    "    except:\n",
    "        pass\n",
    "\n",
    "if userchoice == \"insert\":\n",
    "    print(insert())\n",
    "elif userchoice == \"update\":\n",
    "    print(update())\n",
    "elif userchoice == \"delete\":\n",
    "    print(delete())\n",
    "elif userchoice == \"read\":\n",
    "    read()\n",
    "elif userchoice == \"filter\":\n",
    "    filtering()\n",
    "else:\n",
    "    pass"
   ]
